{
  "name": "rdce",
  "version": "0.3.0",
  "description": "Runtime dead code elimination for JavaScript",
  "main": "lib/rdce.js",
  "bin": {
    "rdce": "bin/rdce"
  },
  "scripts": {
    "build": "tsc --module commonjs --outDir lib/ src/*",
    "watch": "tsc --watch --module commonjs --outDir lib/ src/*"
  },
